@model PageModel
@{ 
    Layout = "~/Views/Shared/_AccountLayout.cshtml";
}

@section scripts{
    <script src="/Content/themes/t/scripts/swfobject.js" type="text/javascript"></script>
    <script src="/Content/themes/t/scripts/components.js" type="text/javascript"></script>
    <script src="/Content/themes/t/scripts/dealonline.js" type="text/javascript"></script>
}
<div class="sec_pan" id="cancelPaySerialNo">
    <form onsubmit="return false;">
    <div class="title-box">
        <h3>
            在线撤消</h3>
    </div>
    <div class="">
        <div class="">
            @Html.Partial("cancelPayProcessing", "deallogselector")
        </div>
        <fieldset>
            <legend>在线撤消</legend>
            <div class="mainInput">
                <div class="fieldInput">
                    <label class="large" for="serialNo">
                        参考号:</label>
                    <input type="text" name="serverSerialNo" id="serverSerialNo" class="t_ipt_fc largeInput focused" />
                </div>
            </div>
            <div id="deal">
                @Html.Action("DealLog")
            </div>
        </fieldset>
    </div>
    <div class="buttons rtl clearfix">
        <a id="next" href="CancelPayAccountPage" class="post large disabled" target="#secbox">
            下一步</a>
    </div>
    <div class="point_list">
        <h3>
            用户提示</h3>
        <ol class="list_hasnum">
            <li>输入参考号，查询商户信息。</li>
            <li>点击下一步。</li>
        </ol>
    </div>
    </form>
</div>
