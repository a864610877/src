@using System.Data
@using Moonlit
@model  RechargingReport
@using (Html.BeginForm("Recharging", "Report"))
{ 
         <div class="place">
    <span>位置：</span>
    <ul class="placeul">
    <li><a href="#">首页</a></li>
    <li><a href="#">@Model.Localize("title")</a></li>
    </ul>
</div>
   <div class="rightinfo">
       
         <table id="selectTd">
           <tr>
                        @Html.EditorForModel("condition")
                        <td>@Html.Submit(Model.Localize("query"))</td>
                       
         </tr>
      </table>
                <div class="searchCommand">@Html.ToolbarOfList(Model.GetToolbarActions())
                </div> 
    <div class="listbox">
       
        @Html.Partial("Templates/datatable", Model)
        @Html.Pager(Model)
    </div>
</div>
}
 <script type="text/javascript">
     function OperatorThis(RName, RUrl) {
         switch (RName) {
             case "RechargingExport":
                 window.location.href = RUrl;
                 break;
         }
     }

</script>